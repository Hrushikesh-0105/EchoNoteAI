{
  "manifest_version": 3,
  "name": "Echo Note Extension",
  "version": "1.0",
  "action": {
    "default_popup": "frontend/popup.html",
    "default_icon": "assets/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://echonote.in/*"]
}
